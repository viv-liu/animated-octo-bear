<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animated_octo_bear"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        
        <activity android:name="com.ui.module.title_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.ui.module.Play_UI"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.ui.module.Import_UI"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.file.process.Import_Process"
            android:label="@string/app_name" >
        </activity>
        
        <activity 
       	android:name="com.play.graphics.Play"
       	android:label="@string/app_name" >
       </activity>
        
        <activity 
       	android:name="com.play.graphics.AnimatedView"
       	android:label="@string/app_name"
       	>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
       </activity>
        
    </application>

</manifest>
